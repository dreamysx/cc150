 * Created by dreamysx on 10/29/16.
 */
public class toStringCount2s {
    int count(int num) {
        int count = 0;
        for (int i=1; i<= num; i++){
            String number = Integer.toString(i);
            for (int j = 0; j < number.length(); j++) {
                String getstr = number.substring(j, j + 1);
                if (getstr.equals("2")) {
                    count++;
                }
            }
        }
        return count;
    }

    public static void main(String[] args) {
        toStringCount2s pj = new toStringCount2s();
        System.out.println(pj.count(87659));
    }
}
